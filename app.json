{
  "expo": {
    "name": "Breathe In",
    "slug": "breatheIn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.breathein.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSContactsUsageDescription": "Breathe In needs access to your contacts to help you find and invite friends.",
        "NSFamilyControlsUsageDescription": "Breathe In uses Screen Time to help you pause and breathe before opening distracting apps."
      },
      "entitlements": {
        "com.apple.developer.family-controls": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "READ_CONTACTS"
      ],
      "package": "com.breathein.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [{ "scheme": "breathein", "host": "overlay", "pathPrefix": "/" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "scheme": "breathein",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/breathe-in-accessibility-plugin.js"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f5d3a5ca-ecfb-4dec-808d-f0d9b6b2778d"
      }
    }
  }
}
